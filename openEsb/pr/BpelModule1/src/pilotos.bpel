<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="pilotos"
    targetNamespace="http://enterprise.netbeans.org/bpel/BpelModule1/pilotos"
    xmlns:tns="http://enterprise.netbeans.org/bpel/BpelModule1/pilotos"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:ns0="http://tempuri.org/">
    <import namespace="http://j2ee.netbeans.org/wsdl/BpelModule1/src/pilotosWSDL" location="pilotosWSDL.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WebService1.asmxWrapper" location="WebService1.asmxWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://tempuri.org/" location="https://localhost:44387/WebService1.asmx?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="WSpilotos" xmlns:tns="http://enterprise.netbeans.org/bpel/WebService1.asmxWrapper" partnerLinkType="tns:WebService1SoapLinkType" partnerRole="WebService1SoapRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/BpelModule1/src/pilotosWSDL" partnerLinkType="tns:pilotosWSDL" myRole="pilotosWSDLPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="AgregarPilotoOut" xmlns:tns="http://tempuri.org/" messageType="tns:AgregarPilotoSoapOut"/>
        <variable name="AgregarPilotoIn" xmlns:tns="http://tempuri.org/" messageType="tns:AgregarPilotoSoapIn"/>
        <variable name="HelloWorldOut" xmlns:tns="http://tempuri.org/" messageType="tns:HelloWorldSoapOut"/>
        <variable name="HelloWorldIn" xmlns:tns="http://tempuri.org/" messageType="tns:HelloWorldSoapIn"/>
        <variable name="PilotosWSDLOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/BpelModule1/src/pilotosWSDL" messageType="tns:pilotosWSDLOperationResponse"/>
        <variable name="PilotosWSDLOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/BpelModule1/src/pilotosWSDL" messageType="tns:pilotosWSDLOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="pilotosWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/BpelModule1/src/pilotosWSDL" portType="tns:pilotosWSDLPortType" variable="PilotosWSDLOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="PilotosWSDLOperationIn" part="nombre"/>
                <to>$AgregarPilotoIn.parameters/ns0:name</to>
            </copy>
            <copy>
                <from variable="PilotosWSDLOperationIn" part="telefono"/>
                <to>$AgregarPilotoIn.parameters/ns0:tel</to>
            </copy>
            <copy>
                <from variable="PilotosWSDLOperationIn" part="marca"/>
                <to>$AgregarPilotoIn.parameters/ns0:marca</to>
            </copy>
            <copy>
                <from variable="PilotosWSDLOperationIn" part="linea"/>
                <to>$AgregarPilotoIn.parameters/ns0:linea</to>
            </copy>
            <copy>
                <from variable="PilotosWSDLOperationIn" part="placa"/>
                <to>$AgregarPilotoIn.parameters/ns0:placa</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="WSpilotos" operation="AgregarPiloto" xmlns:tns="http://tempuri.org/" portType="tns:WebService1Soap" inputVariable="AgregarPilotoIn" outputVariable="AgregarPilotoOut"/>
        <assign name="Assign2">
            <copy>
                <from>$AgregarPilotoOut.parameters/ns0:AgregarPilotoResult</from>
                <to variable="PilotosWSDLOperationOut" part="part1"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="pilotosWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/BpelModule1/src/pilotosWSDL" portType="tns:pilotosWSDLPortType" variable="PilotosWSDLOperationOut"/>
    </sequence>
</process>
